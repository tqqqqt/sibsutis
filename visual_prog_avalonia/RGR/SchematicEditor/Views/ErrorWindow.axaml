<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="100"
		MinWidth="400" MaxWidth="400" Width="400" 
		MinHeight="100" MaxHeight="100" Height="100"
        x:Class="SchematicEditor.Views.ErrorWindow"
        Title="Error Window">
	
  <StackPanel>
	  <TextBlock Text="Ошибка"
				 FontSize="14"
	             HorizontalAlignment="Center"
				 VerticalAlignment="Center"
				 Margin="10,10,10,10"/>
	  <TextBlock Text="{Binding ErrorMesage}"
				 FontSize="14"
				 HorizontalAlignment="Center"
				 VerticalAlignment="Center"
				 Margin="10,10,10,10"/>
  </StackPanel>
</Window>
